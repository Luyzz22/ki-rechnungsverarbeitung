  <style id="sbs-shared-layout">
:root{
  --sbs-bg:#f5f6f8;
  --sbs-white:#ffffff;
  --sbs-dark:#003856;
  --sbs-dark-soft:#0b2435;
  --sbs-accent:#ffb400;
  --sbs-border-soft:rgba(15,23,42,0.08);
  --font-sans:-apple-system,BlinkMacSystemFont,system-ui,-system-ui,-system,sans-serif;
  --transition-fast:all .25s ease;
}
*{box-sizing:border-box;}
body{
  margin:0;
  padding:0;
  font-family:var(--font-sans);
  background:var(--sbs-bg);
  color:#17212b;
  -webkit-font-smoothing:antialiased;
}
a{text-decoration:none;color:inherit;}
.page-wrap{
  min-height:100vh;
  display:flex;
  flex-direction:column;
}
main.sbs-page-main{
  flex:1;
  padding:56px 0 40px;
}
.container{
  max-width:1200px;
  margin:0 auto;
  padding:0 24px;
}
.sbs-page-content{
  max-width:900px;
  margin:0 auto;
  background:#ffffff;
  padding:32px 28px 28px;
  border-radius:24px;
  box-shadow:0 10px 30px rgba(15,23,42,0.06);
}
.sbs-page-content h1,
.sbs-page-content h2,
.sbs-page-content h3{
  color:var(--sbs-dark);
}


.sbs-logo-wrap{
  display:flex;
  align-items:center;
  gap:14px;
}
.sbs-logo-img{
  height:46px;
  width:auto;
  display:block;
}
.sbs-logo-text{
  display:flex;
  flex-direction:column;
  font-size:13px;
  line-height:1.25;
  color:var(--sbs-dark-soft);
}
.sbs-logo-text strong{
  font-size:14px;
  letter-spacing:.03em;
  text-transform:uppercase;
}
.sbs-nav{
  display:flex;
  align-items:center;
  gap:26px;
  font-size:15px;
  color:var(--sbs-dark-soft);
}
.sbs-nav a{
  position:relative;
  padding-bottom:4px;
  opacity:.85;
  transition:var(--transition-fast);
}
.sbs-nav a:hover{
  opacity:1;
}
.sbs-nav a.active::after,
.sbs-nav a:hover::after{
  content:"";
  position:absolute;
  left:0;
  bottom:0;
  width:100%;
  height:2px;
  border-radius:999px;
  background:var(--sbs-accent);
}
.sbs-nav-cta{
  padding:9px 16px;
  border-radius:999px;
  background:var(--sbs-accent);
  color:#111827;
  font-weight:600;
  box-shadow:0 8px 18px rgba(255,180,0,.32);
}
.sbs-nav-cta:hover{
  transform:translateY(-1px);
  box-shadow:0 12px 26px rgba(255,180,0,.4);
}
#sbs-footer-global{
  border-top:1px solid rgba(148,163,253,.16);
  background:#fff;
  padding:16px 24px 18px;
  font-size:12px;
  color:#4b5563;
  margin-top:40px;
}
#sbs-footer-global .footer-inner{
  max-width:1200px;
  margin:0 auto;
  display:flex;
  flex-wrap:wrap;
  align-items:center;
  justify-content:space-between;
  gap:10px;
}
#sbs-footer-global .footer-links{
  display:flex;
  flex-wrap:wrap;
  gap:14px;
}
#sbs-footer-global a{
  color:#111827;
  opacity:.9;
  transition:var(--transition-fast);
}
#sbs-footer-global a:hover{
  opacity:1;
  color:var(--sbs-accent);
}
@media(max-width:640px){
  
  .sbs-nav{
    flex-wrap:wrap;
    gap:16px;
    padding-bottom:4px;
  }
  .sbs-page-content{
    padding:24px 18px 22px;
  }
}

    /* ================================================
       EINHEITLICHES HEADER/FOOTER DESIGN
